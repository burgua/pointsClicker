{
    "manifest_version": 2,
    "name": "Twitch points clicker",
    "description": "Automatically claims twitch points reward",
    "version": "0.3",
    "icons": {
        "128": "icon_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.twitch.tv/*"
            ],
            "js": ["code.js"]
        }
    ]
}
